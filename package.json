{
    "name": "crackbuster",
    "version": "1.0.0",
    "description": "Foundation Crack Repair website for Edmonton, Canada",
    "main": "server/index.js",
    "scripts": {
        "dev": "concurrently \"npm run dev:server\" \"npm run dev:client\"",
        "dev:server": "nodemon server/index.js",
        "dev:client": "webpack serve --mode development --config webpack.config.js",
        "build": "npm run build:client && npm run build:server",
        "build:client": "webpack --mode production --config webpack.config.js",
        "build:server": "webpack --mode production --config webpack.server.config.js",
        "build:watch": "concurrently \"npm run build:watch:client\" \"npm run build:watch:server\"",
        "build:watch:client": "webpack --mode development --config webpack.config.js --watch",
        "build:watch:server": "webpack --mode development --config webpack.server.config.js --watch",
        "start": "node dist/server/server.js",
        "seed": "node scripts/seed-database.js",
        "seed:seo": "node scripts/seed-seo.js",
        "optimize:images": "node scripts/optimize-images-to-webp.js"
    },
    "keywords": [
        "foundation",
        "crack",
        "repair",
        "edmonton",
        "canada"
    ],
    "author": "",
    "license": "ISC",
    "dependencies": {
        "dotenv": "^16.3.1",
        "express": "^4.18.2",
        "express-basic-auth": "^1.2.1",
        "form-data": "^4.0.5",
        "mailgun.js": "^12.4.0",
        "mongoose": "^8.0.3",
        "multer": "^2.0.2",
        "puppeteer": "^24.34.0",
        "quill": "^2.0.3",
        "react": "^18.2.0",
        "react-dom": "^18.2.0",
        "react-helmet-async": "^2.0.4",
        "react-quill": "^2.0.0",
        "react-router-dom": "^6.21.1",
        "sharp": "^0.34.5"
    },
    "devDependencies": {
        "@babel/core": "^7.23.6",
        "@babel/preset-env": "^7.23.6",
        "@babel/preset-react": "^7.23.5",
        "@babel/register": "^7.28.3",
        "babel-loader": "^9.1.3",
        "concurrently": "^8.2.2",
        "copy-webpack-plugin": "^13.0.1",
        "css-loader": "^6.8.1",
        "file-loader": "^6.2.0",
        "html-webpack-plugin": "^5.6.0",
        "ignore-loader": "^0.1.2",
        "mammoth": "^1.11.0",
        "mini-css-extract-plugin": "^2.7.6",
        "nodemon": "^3.0.2",
        "sass": "^1.69.5",
        "sass-loader": "^13.3.2",
        "style-loader": "^3.3.3",
        "url-loader": "^4.1.1",
        "webpack": "^5.89.0",
        "webpack-cli": "^5.1.4",
        "webpack-dev-server": "^4.15.1",
        "webpack-node-externals": "^3.0.0"
    }
}
